@page "/"
@using WebApp.Controls
@model ControlInicio
@{
    ViewData["Title"] = "Inicio";
    Database.Out @out = new();
    Database.Gather g = new();
    g.ForwardOnline();
    @out.Session.Add(1);
    int id = @out.Session.Last();
    var go = @out.CallLoginCheck(id);
}

